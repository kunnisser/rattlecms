   <!-- Load javascripts at bottom, this will reduce page load time -->
   <script src="<%= baseUrl%>plugins/js/jquery-1.8.3.min.js"></script>
   <script src="<%= baseUrl%>plugins/js/mock.js"></script>   
   <script src="<%= baseUrl%>plugins/assets/bootstrap/js/bootstrap.min.js"></script>
   <script src="<%= baseUrl%>plugins/assets/bootstrap-wizard/jquery.bootstrap.wizard.min.js"></script>
	 <script src="<%= baseUrl%>plugins/js/crop/iscroll-zoom.js"></script>
 	 <script src="<%= baseUrl%>plugins/js/crop/hammer.js"></script>
	 <script src="<%= baseUrl%>plugins/js/crop/jquery.photoClip.js"></script>
   <!-- ie8 fixes -->
   <!--[if lt IE 9]>
   <script src="js/excanvas.js"></script>
   <script src="js/respond.js"></script>
   <![endif]-->
   <script src="<%= baseUrl%>plugins/js/scripts.js"></script>   
   <script>
      jQuery(document).ready(function() {       
         // initiate layout and plugins
         App.init();
				//crop init
				$("#clipArea").photoClip({
					width: 200,
					height: 200,
					file: "#file",
					view: "#view",
					ok: "#clipBtn",
					loadStart: function() {
						console.log("照片读取中");
					},
					loadComplete: function() {
						console.log("照片读取完成");
					},
					clipFinish: function(dataURL) {
						console.log(dataURL);
					}
				});
      });
   </script>
</body>
